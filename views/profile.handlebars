{{#if posts.length}}
<div class="container">
  <h2>Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}
    
</div>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/profile/new">+ New Post</a></button>

<script src="./js/add-post.js"></script>

